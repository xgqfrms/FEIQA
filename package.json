{
    "name": "feiqa",
    "version": "1.1.1",
    "description": "ES6/7/8 to ES5",
    "module": "webpack.config.mjs",
    "main": "webpack.config.js",
    "scripts": {
        "app": "npm run rmrf && webpack -p",
        "prod": "webpack -p",
        "dev": "npm run rmrf & webpack -w",
        "lint": "eslint ./src/*.js",
        "fix": "eslint --fix ./src/*.js",
        "lint_test": "eslint ./src/test.js",
        "lint_fix": "eslint --fix ./src/test.js",
        "rmrf": "rimraf build",
        "rmrf-js-css": "rimraf build/js & rimraf build/css",
        "rmrf-all": "rimraf build/*",
        "index": "cd build && browser-sync start -s -f './*.*'",
        "json_db": "cd json && json-server --watch db.json --port 7777",
        "linux_rmrf": "rm -rf build/public",
        "windows_clear": "rimraf build/public"
    },
    "bin": {
        "nct": "nctcli",
        "bin": "./bin.js"
    },
    "keywords": [
        "webapck",
        "auto_transpile",
        "es6/7/8",
        "es5"
    ],
    "author": "xgqfrms, gildata",
    "license": "MIT",
    "dependencies": {
        "browserify": "^14.5.0",
        "requirejs": "^2.3.5",
        "systemjs": "^0.20.19",
        "webpack": "^3.8.1",
        "babel-polyfill": "^6.26.0",
        "whatwg-fetch": "^2.0.3"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-es2015-destructuring": "^6.23.0",
        "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
        "babel-plugin-transform-es2015-spread": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "browser-sync": "^2.23.6",
        "clean-webpack-plugin": "^0.1.17",
        "colors": "^1.1.2",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.7",
        "dotenv": "^4.0.0",
        "es-module-loader": "^2.2.8",
        "eslint": "^4.16.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "html-minifier": "^3.5.8",
        "html-webpack-plugin": "^2.30.1",
        "http-server": "^0.11.1",
        "json-server": "^0.12.1",
        "json5": "^0.5.1",
        "node-fetch": "^2.6.1",
        "node-sass": "^4.7.2",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "url-loader": "^0.6.2"
    }
}
